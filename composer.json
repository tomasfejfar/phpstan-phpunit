{
    "name": "tomasfejfar/phpstan-phpunit",
    "type": "phpstan-plugin",
    "description": "Collection of phpstan rules for phpunit",
    "require": {
        "phpstan/phpstan": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tomasfejfar\\PhpstanPhpunit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tomasfejfar\\PhpstanPhpunit\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tomáš Fejfar",
            "email": "tomas.fejfar@gmail.com"
        }
    ],
    "scripts": {
        "build": [
            "@composer validate --strict",
            "@test"
        ],
        "test": "phpunit"
    },
    "config": {
        "lock": false,
        "sort-packages": true
    }
}
